# Node modules - avoid pushing dependencies to the repo
node_modules/

# Vite build output
dist/

# Build runner - ignore the build server runner
servebuild.cjs

# Local env files - keep sensitive data out of version control
.env
.env.local
.env.*.local

# Logs - ignore package manager logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# IDE directories - IDE-specific settings
.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?

# TypeScript cache
*.tsbuildinfo

# Optional npm cache directory
.npm

# Testing coverage - generated by testing tools
coverage/

# Jest cache and output
jest/
jest.config.js

# Storybook build output - Storybook static assets
storybook-static/

# ESLint cache
.eslintcache

# Lint-staged config
lint-staged.config.js

# Husky - pre-commit hooks
.husky/_

# Ignore Prettier ignore config (optional, if you use one)
.prettierignore

# Optional lock files - if you use more than one package manager, ignore lock files you don't use
# (Comment out the ones you're using)
# package-lock.json
# yarn.lock
# pnpm-lock.yaml

# MacOS - system files that should not be in version control
.DS_Store

# Windows - OS-generated files
Thumbs.db
ehthumbs.db

# Linux - lockfiles (optional)
*.lock
